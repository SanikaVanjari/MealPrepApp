<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_latest_recipe"
        android:name="com.practice.mealprepapp.ui.home.LatestRecipeFragment"
        android:label="@string/latest_recipe"
        tools:layout="@layout/fragment_latest_recipe" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.practice.mealprepapp.ui.dashboard.DashboardFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.practice.mealprepapp.ui.notifications.SearchFragment"
        android:label="@string/search"
        tools:layout="@layout/fragment_search" />
</navigation>